<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    OWASP Dependency Check Suppressions
    
    Add suppressions here for false positives or accepted risks.
    Each suppression should include:
    - A clear reason for suppression
    - The specific CVE or vulnerability pattern
    - An expiration date for review
    -->
    
    <!-- Example suppression for false positives -->
    <!--
    <suppress>
        <notes><![CDATA[
        This suppresses false positives for Spring Boot test dependencies
        that are not used in production. Review by 2024-12-31.
        ]]></notes>
        <cve>CVE-2021-22118</cve>
        <filePath regex="true">.*spring-boot-test.*\.jar</filePath>
    </suppress>
    -->
    
    <!-- Suppress development/test only dependencies -->
    <suppress until="2024-12-31Z">
        <notes><![CDATA[
        H2 database is only used for development and testing, not in production.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*</packageUrl>
    </suppress>
    
    <!-- Example: Suppress specific CVE if it doesn't apply to your usage -->
    <!--
    <suppress until="2024-12-31Z">
        <notes><![CDATA[
        CVE-2021-44228 (Log4Shell) - We are using Logback, not Log4j.
        This is a false positive.
        ]]></notes>
        <cve>CVE-2021-44228</cve>
    </suppress>
    -->
</suppressions>
